{
    "agent": {
        "metrics_collection_interval": 60
    },
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/opt/cloudformation/cloudwatch/cloudwatch-logging.log",
                        "log_group_name": "vlad-cloudwatch-logging-log-group",
                        "log_stream_name": "vlad-cloudwatch-logging-log-stream",
                        "timezone": "UTC",
                        "timestamp_format": "%Y-%m-%d %H:%M:%S"
                    }
                ]
            }
        },
        "log_stream_name": "vlad-cloudwatch-logging-default-log-stream"
    }
}
